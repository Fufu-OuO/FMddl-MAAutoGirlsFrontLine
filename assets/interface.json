{
    "name": "FMddl-MAA",
    "version":  "v0.1.0",
    "mirrorchyan_rid":  "FMddl-MAAutoGirlsFrontLine",
    "mirrorchyan_multiplatform":  false, 
    "url":  "https://github.com/Fufu-OuO/FMddl-MAAutoGirlsFrontLine",
    "custom_title": "费马 FMddl-MAA",
    "controller": [
        {
            "name": "Steam",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "少女前线"
            }
        }
    ],
    "resource": [
        {
            "name": "Steam 官服",
            "path": [
                "{PROJECT_DIR}/resource/SteamGF"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "登录游戏",
            "entry": "EnterGame",
            "option": [
                "服务器"
            ]
        },
        {
            "name": "模拟作战（人形材料）",
            "entry": "DailyA",
            "option": [
                "人形材料关卡",
                "模拟作战难度",
                "模拟作战次数"
            ]
        },
        {
            "name": "融合演习（融合材料）",
            "entry": "DailyB",
            "option": [
                "融合材料关卡",
                "融合演习次数"
            ]
        },
        {
            "name": "每日任务",
            "entry": "DailyTask",
            "option": [
                "[其他] 送经验书×1"
            ]
        },
        {
            "name": "[溯源回归] 当期活动打捞",
            "entry": "EventSalvage",
            "option": [
                "活动打捞人形",
                "活动打捞作战难度",
                "活动打捞出战梯队",
                "活动打捞梯队所部署的机场位置"
            ]
        },
        {
            "name": "宿舍（好感、点赞、电池）",
            "entry": "RoomDorm",
            "option": [
                "好友点赞"
            ]
        },
        {
            "name": "资料室（人形经验书）",
            "entry": "RoomData"
        },
        {
            "name": "前进营地（已出发则不操作）",
            "entry": "RoomExplore"
        },
        {
            "name": "领取奖励",
            "entry": "Rewardget"
        }
    ],
